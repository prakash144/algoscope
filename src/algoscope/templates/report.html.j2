    // Premium Theme toggle (light/dark)
    const toggleTheme = document.getElementById('toggleTheme');
    let dark = true;
    toggleTheme.addEventListener('click', () => {
      dark = !dark;
      if (!dark) {
        // Light theme
        document.documentElement.style.setProperty('--bg-primary','#ffffff');
